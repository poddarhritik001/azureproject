{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "hriadf"
		},
		"Azure Sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Azure Sql'"
		},
		"Data Lake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Data Lake'"
		},
		"Azure Sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "hpserverr.database.windows.net"
		},
		"Azure Sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "hpdatabase"
		},
		"Azure Sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Hritik"
		},
		"Data Lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hritikpstorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "hriadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azure Sql_password": {
						"value": "[parameters('Azure Sql_password')]"
					},
					"Data Lake_accountKey": {
						"value": "[parameters('Data Lake_accountKey')]"
					},
					"Azure Sql_properties_typeProperties_server": {
						"value": "[parameters('Azure Sql_properties_typeProperties_server')]"
					},
					"Azure Sql_properties_typeProperties_database": {
						"value": "[parameters('Azure Sql_properties_typeProperties_database')]"
					},
					"Azure Sql_properties_typeProperties_userName": {
						"value": "[parameters('Azure Sql_properties_typeProperties_userName')]"
					},
					"Data Lake_properties_typeProperties_url": {
						"value": "[parameters('Data Lake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}